{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build && node .",
    "start_dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev": "npm run build & ts-node-dev --respawn --transpile-only src/index.ts",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && tsc && node node_modules/copyfiles/copyfiles package.json ./lib && node node_modules/copyfiles/copyfiles arena.env ./lib",
    "clean": "node node_modules/rimraf/bin lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "0.15.2",
    "@types/cors": "2.8.17",
    "@types/express": "^4.17.21",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "rimraf": "2.7.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@colyseus/monitor": "0.15.4",
    "@colyseus/social": "0.11.7",
    "@colyseus/tools": "0.15.26",
    "@dcl-sdk/utils": "^1.1.3",
    "@mikro-orm/core": "4.5.10",
    "@mikro-orm/mongodb": "4.5.10",
    "axios": "1.6.2",
    "bcrypt": "^5.1.1",
    "bip39": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "colyseus": "0.15.13",
    "core-js": "3.33.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "node-fetch": "^3.3.2",
    "open-gamefi": "0.0.3",
    "path": "^0.12.7",
    "stellar-sdk": "^12.2.0",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.1.8",
    "url-polyfill": "1.1.12"
  }
}
